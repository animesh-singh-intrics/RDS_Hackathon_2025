    "lucide-react": "^0.544.0",
    "postcss": "^8.5.2",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.55.1",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^7.1.1",
    "tailwindcss": "^3.4.17"
